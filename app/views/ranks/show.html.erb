<div class='w-full h-full flex flex-col '>
  <%= link_to 'Back', books_path%>
   <section>
      <h1>Date</h1>
      <div>
         <% @ranks.each do |rank|%>
         <a href='<%= rank_path(rank)%>'> Date: <%= rank.date %> </a>
         <%end%>
      </div>
      <h1>Ranking</h1>
   </section>
   <section>
      <%@book_ranks.each_with_index do |book,index|%>
         <p>NO: <%=index+1%> <%= link_to book.book.name, book_path(book.book)%> views: <%=book.view%></p>
      <%end%>
   </section>
</div>
